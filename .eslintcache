         <FunctionalComponent\n            string=\"Background Ccolor Toggle\"\n            function={toggleBackgroundColor}\n            selectedStyle={backgroundColor}\n          />\n          <FunctionalComponent\n            string=\"Border Radius Toggle\"\n            function={toggleBorderRadius}\n            selectedStyle={borderRadius}\n          />\n          <FunctionalComponent\n            string=\"Border Style Toggle\"\n            function={toggleBorderStyle}\n            selectedStyle={borderStyle}\n          />\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default PropsDemo;\n\nconst FunctionalComponent = (props) => {\n  return (\n    <div>\n      <p>{props.string}</p>\n      <button onClick={props.function}>Press Me!</button>\n      <TinyComponent selectedStyle={props.selectedStyle} />\n    </div>\n  );\n};\n\nconst TinyComponent = (props) => {\n  return (\n    <div>\n      <p>The current style is : {props.selectedStyle}</p>\n    </div>\n  );\n};\n\nFunctionalComponent.defaultProps = {\n  string: 'This is wild!',\n  function: () => console.log('Can I see this in my dev tools?'),\n  selectedStyle: 'What style??',\n};\n\nFunctionalComponent.propTypes = {\n  string: PropTypes.string.isRequired,\n  function: PropTypes.func.isRequired,\n  selectedStyle: PropTypes.string.isRequired,\n};\n","C:\\Users\\Ryan\\Desktop\\react-fundamentals\\src\\components\\concepts\\Hooks.js",["82"],"import React, { useState, useEffect } from 'react';\n\nconst Hooks = () => {\n  const [query, setQuery] = useState('');\n  const [results, setResults] = useState({});\n\n  const fetcher = () => {\n    fetch(`https://swapi.co/api/people/${query}`)\n      .then((res) => res.json())\n      .then((json) => {\n        console.log(json);\n        setResults(json);\n      });\n  };\n\n  return (\n    <div className=\"main\">\n      <div className=\"mainDiv\">\n        <input\n          value={query}\n          onChange={(e) => setQuery(e.target.value)}\n          placeholder=\"enter your star wars character number\"\n        />\n        <button onClick={() => fetcher()}>Click for Character!</button>\n        {results ? <h2>{results.name}</h2> : <div></div>}\n      </div>\n    </div>\n  );\n};\n\nconst Hooks2 = () => {\n  const [results, queryNum, setQueryNum] = useNumHook('');\n  const [clicks, setClicks] = useClicks(0);\n\n  return (\n    <div className=\"main\">\n      <div className=\"mainDiv\">\n        <h3>Enter a number below to see a number fact.</h3>\n        <input\n          value={queryNum}\n          onChange={(e) => setQueryNum(e.target.value)}\n          placeholder=\"enter a number\"\n        />\n        {results ? <h2>{results}</h2> : <div></div>}\n        <button onClick={() => setClicks(clicks + 1)}>\n          Click to update document title\n        </button>\n      </div>\n    </div>\n  );\n};\n\nconst useNumHook = (num) => {\n  const [queryNum, setQueryNum] = useState(num);\n  const [results, setResults] = useState('');\n\n  useEffect(() => {\n    if (queryNum !== '') {\n      fetch(`http://numbersapi.com/${queryNum}`)\n        .then((res) => res.text())\n        .then((json) => {\n          console.log(json);\n          setResults(json);\n        });\n    }\n  }, [queryNum]);\n\n  return [results, queryNum, setQueryNum];\n};\n\nconst useClicks = (initCount) => {\n  const [clicks, setClicks] = useState(initCount);\n\n  useEffect(() => {\n    document.title = `You have clicked ${clicks} times`;\n  }, [clicks]);\n\n  return [clicks, setClicks];\n};\n\nexport default Hooks2;\n","C:\\Users\\Ryan\\Desktop\\react-fundamentals\\src\\components\\apps\\timer-apps\\TimePiecesApp.js",[],"C:\\Users\\Ryan\\Desktop\\react-fundamentals\\src\\components\\apps\\timer-apps\\TimerApp.js",[],"C:\\Users\\Ryan\\Desktop\\react-fundamentals\\src\\components\\apps\\timer-apps\\ClockApp.js",[],"C:\\Users\\Ryan\\Desktop\\react-fundamentals\\src\\components\\apps\\timer-apps\\StopWatchApp.js",[],{"ruleId":"83","replacedBy":"84"},{"ruleId"